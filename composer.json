{
    "name": "fnbr/webtool",
    "version": "3.8",
    "description": "FrameNet Brasil Web Annotation Tool",
    "license": "GPLv3",
    "keywords": [
        "framenet",
        "annotation",
        "frames",
        "constructions"
    ],
    "authors": [
        {
            "name": "Tiago Torrent",
            "email": "tiago.torrent@gmail.com"
        },
        {
            "name": "Ely Matos",
            "email": "admin@ematos.net"
        },
        {
            "name": "fnzr",
            "email": "admin@fmatos.net"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/elymatos/laravelcollective-annotations.git"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "auth0/auth0-php": "^8.0",
        "claviska/simpleimage": "^2.5",
        "doctrine/collections": "^2.2",
        "google/cloud-speech": "^1.19",
        "google/cloud-storage": "^1.42",
        "guzzlehttp/guzzle": "^7.2",
        "james-heinrich/getid3": "^1.9",
        "laravel/framework": "^12.0",
        "laravel/octane": "^2.4",
        "laravel/reverb": "@beta",
        "laravel/tinker": "^2.9",
        "laudis/neo4j-php-client": "^2.6",
        "openai-php/laravel": "^0.11.0",
        "php-ffmpeg/php-ffmpeg": "^1.2",
        "phplrt/runtime": "^3.6",
        "rdx/laravelcollective-annotations": "dev-main",
        "spatie/laravel-data": "*",
        "textalk/websocket": "^1.5",
        "thiagoalessio/tesseract_ocr": "^2.13",
        "vlucas/phpdotenv": "^5.4",
        "pestphp/pest": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "greenlion/php-sql-parser": "^4.5",
        "laravel/boost": "^1.3",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpfastcache/phpfastcache": "^9",
        "phplrt/phplrt": "^3.6",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "files": [
            "bootstrap/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
